pipeline {
    agent any
    tools {
        nodejs '20.9.0'
        // nodejs '14.17.3'
    }
    stages {
        stage('Hello from echo') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hello from node') {
            steps {
                sh 'node app.js'
            }
        }
    }
}
